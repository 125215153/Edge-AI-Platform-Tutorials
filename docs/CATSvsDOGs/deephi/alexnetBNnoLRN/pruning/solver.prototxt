momentum: 0.9
weight_decay: 0.0005

snapshot: 500

snapshot_prefix: "deephi/alexnetBNnoLRN/pruning/snapshot_"

solver_mode: GPU

#type: "SGD"
type: "Adam"

random_seed: 1201

